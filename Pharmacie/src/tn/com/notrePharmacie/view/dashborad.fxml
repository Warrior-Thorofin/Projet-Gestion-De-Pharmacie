<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="400" prefWidth="600" type="AnchorPane" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.com.notrePharmacie.controllerDashboardAdminController">

    <children>
        <Button layoutX="240.0" layoutY="14.0" onAction="#openProduits" prefHeight="26.0" prefWidth="83.0" text="Produits" />
        <Button layoutX="239.0" layoutY="54.0" onAction="#openCommandes" text="Commandes" />
        <Button fx:id="historiqueButton" layoutX="239.0" layoutY="173.0" onAction="#openHistorique" prefHeight="26.0" prefWidth="83.0" text="Historique " />
        <Button fx:id="UtilisateursButton" layoutX="239.0" layoutY="210.0" onAction="#openUsers" prefHeight="26.0" prefWidth="83.0" text="Utilisateurs" />
        <Button layoutX="240.0" layoutY="94.0" onAction="#alerte" prefHeight="26.0" prefWidth="83.0" text="Fin de stock" />
      <Button layoutX="240.0" layoutY="333.0" onAction="#logout" prefHeight="26.0" prefWidth="83.0" text="Logout" />
      <Button fx:id="performanceButton" layoutX="239.0" layoutY="293.0" onAction="#perfermance" prefHeight="26.0" prefWidth="83.0" text="Perfermance" visible="true" />
      <Button fx:id="chiffreAffaireButton" layoutX="239.0" layoutY="253.0" onAction="#chiffreAffaire" prefHeight="26.0" prefWidth="83.0" text="ChiffreAffaire" visible="true" />
      <Button layoutX="239.0" layoutY="134.0" onAction="#openFournisseurs" prefHeight="26.0" prefWidth="83.0" text="Fournisseur" />
    </children>

</fx:root>
